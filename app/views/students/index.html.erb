<div class="container">
  <ul>
    <li><%= link_to "Export CSV", students_path(format: :csv) %></li>
    <li><a href="/">Home</a></li>
  </ul>
  <h1>CSV Export Sample Student</h1>
  <table>
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Date of Birth</th>
        <th scope="col">Email</th>
      </tr>
    </thead>
    <tbody>
      <% @students.each do |student| %>
        <tr>
          <td><%= student.full_name%></td>
          <td><%= student.DOB%></td>
          <td><%= student.email%></td>
        </tr>
      <% end %>
      <tr class="spacer"></tr>
    </tbody>
  </table>
</div>